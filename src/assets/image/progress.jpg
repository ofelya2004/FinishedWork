    .divider
    {
        position: relative;
        margin-top: 50px;
        height: 1px;
    }

    .div-transparent:before
    {
        content: "";
        position: absolute;
        top: 0;
        left: 5%;
        right: 5%;
        width: 90%;
        height: 1px;
        background-image: linear-gradient(to right, transparent, grey, transparent);
    }

    .div-arrow-down:after {
        content: "";
        position: absolute;
        z-index: 1;
        top: -7px;
        left: calc(50% - 7px);
        width: 14px;
        height: 14px;
        transform: rotate(45deg);
        background-color: white;
        border-bottom: 1px solid grey;
        border-right: 1px solid grey;
    }

    .div-tab-down:after {
        content: "";
        position: absolute;
        z-index: 1;
        top: 0;
        left: calc(50% - 10px);
        width: 20px;
        height: 14px;
        background-color: white;
        border-bottom: 1px solid grey;
        border-left: 1px solid grey;
        border-right: 1px solid grey;
        border-radius: 0 0 8px 8px;
    }

    .div-stopper:after {
        content: "";
        position: absolute;
        z-index: 1;
        top: -6px;
        left: calc(50% - 7px);
        width: 14px;
        height: 12px;
        background-color: white;
        border-left: 1px solid grey;
        border-right: 1px solid grey;
    }

    .div-dot:after {
        content: "";
        position: absolute;
        z-index: 1;
        top: -9px;
        left: calc(50% - 9px);
        width: 18px;
        height: 18px;
        background-color: #2a6496;
        border: 1px solid grey;
        border-radius: 50%;
        box-shadow: inset 0 0 0 2px white, 0 0 0 4px white;
    }
</style>
